<CheckCase name="2D(.VSD) - 2D(.VSD)"  sourceA="VSD" sourceB="VSD">

<Check type="Comparison" sourceAType="VSD" sourceBType="VSD">
	<ComponentGroup sourceAGroupName="SCNOZZ"  sourceBGroupName="SCNOZZ">
			  
			  <ComponentClass sourceAComponentClass="SCNOZZ"  sourceBComponentClass="SCNOZZ">
			      <Matchwith sourceAPropertyname="NAME"  sourceBPropertyname ="NAME"/>
