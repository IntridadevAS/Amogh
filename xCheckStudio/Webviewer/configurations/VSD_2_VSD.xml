				  <Property sourceAName="NAME" sourceBName="NAME"  severity="Error"/>				  
			      <Property sourceAName="ActType" sourceBName="ActType"  severity="Error"/>
				  <Property sourceAName="OWNER" sourceBName="OWNER"  severity="Error"/>
				  <Property sourceAName="Text" sourceBName="Text"  severity="Error"/>
