			<Property name="FUNC" rule="Must have value" severity="Error" comment="Property must have value." />
			<Property name="BORE" rule="Must have value" severity="Error" comment="Property must have value." />
            <Property name="CATR" rule="Must have value" severity="Error" comment="Property must have value." />
