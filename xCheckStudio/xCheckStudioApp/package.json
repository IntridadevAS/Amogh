{
	"name": "xcheckstudio",
	"author": "XcheckStudio",
	"version": "1.0.0",
	"main": "main.js",
	"devDependencies": {
		"@types/mocha": "^8.2.1",
		"@wdio/selenium-standalone-service": "^6.11.0",
		"chai": "^4.3.3",
		"chai-as-promised": "^7.1.1",
		"electron": "^12.0.0",
		"electron-builder": "^20.40.2",
		"electron-chromedriver": "^11.0.0",
		"mocha": "^8.3.0",
		"spectron": "^13.0.0"
	},
	"scripts": {
		"start": "electron .",
		"test": "mocha",
		"dist": "build"
	},
	"dependencies": {
		"ajax-request": "^1.2.3",
		"electron-dl": "^3.0.0",
		"electron-pdf-window": "^1.0.12",
		"exceljs": "^4.1.1",
		"xlsx": "^0.14.3"
	},
	"build": {
		"appId": "com.intrida.xCheckStudioApp",
		"productName": "XcheckStudio",
		"win": {
			"target": [
				"nsis"
			],
			"icon": "build/main_256x256.ico"
		},
		"nsis": {
			"oneClick": false,
			"installerIcon": "build/main_32x32.ico",
			"uninstallerIcon": "build/main_32x32.ico",
			"installerSidebar": "build/banner.bmp",
			"uninstallerSidebar": "build/banner.bmp",
			"runAfterFinish": false,
			"allowToChangeInstallationDirectory": true,
			"perMachine": true,
			"deleteAppDataOnUninstall": true,
			"createDesktopShortcut": "always",
			"createStartMenuShortcut": true,
			"menuCategory": true
		}
	}
}
