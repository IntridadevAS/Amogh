{
	"name": "xcheckstudio",
	"version": "1.0.0",
	"main": "main.js",
	"devDependencies": {
		"electron": "^3.0.6",
		"electron-builder": "^20.40.2"
	},
	"scripts": {
		"start": "electron .",
		"dist": "build"
	},
	"dependencies": {
		"ajax-request": "^1.2.3",
		"electron-dl": "^3.0.0",
		"electron-pdf-window": "^1.0.12",
		"xlsx": "^0.14.3"
	},
	"build": {
		"appId": "com.intrida.xCheckStudioApp",
		"productName": "xCheckStudio Client",
		"win": {
			"target": [
				"nsis"
			],
			"icon": "build/main_256x256.ico"
		},
		"nsis": {
			"installerIcon": "build/main_32x32.ico",
